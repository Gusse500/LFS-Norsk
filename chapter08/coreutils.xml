      </varlistentry>

      <varlistentry id="mkdir">
        <term><command>mkdir</command></term>
        <listitem>
          <para>Oppretter en mappe med gitt navn</para>
          <indexterm zone="ch-system-coreutils mkdir">
            <primary sortas="b-mkdir">mkdir</primary>
          </indexterm>
        </listitem>
