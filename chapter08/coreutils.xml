        <listitem>
          <para>Rapporterer den numeriske identifikatoren (i heksadesimal) til verten</para>
          <indexterm zone="ch-system-coreutils hostid">
            <primary sortas="b-hostid">hostid</primary>
          </indexterm>
        </listitem>
      </varlistentry>

      <varlistentry id="id">
        <term><command>id</command></term>
        <listitem>
          <para>Rapporterer effektiv brukerID, gruppeID og gruppemedlemskap
          av gjeldende bruker eller en spesifisert bruker</para>
          <indexterm zone="ch-system-coreutils id">
            <primary sortas="b-id">id</primary>
          </indexterm>
