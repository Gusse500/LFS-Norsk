        <term><command>md5sum</command></term>
        <listitem>
          <para>Rapporterer eller kontrollerer Message Digest 5 (MD5) sjekksummer</para>
          <indexterm zone="ch-system-coreutils md5sum">
            <primary sortas="b-md5sum">md5sum</primary>
